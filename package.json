{
  "name": "otf-api-ts",
  "version": "1.0.0",
  "description": "TypeScript client library for OrangeTheory Fitness API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "download-schema": "node scripts/download-schema.js",
    "generate-types": "npm run download-schema && openapi-typescript schema/openapi.yaml -o src/generated/types.ts",
    "build": "tsc",
    "build-with-schema": "npm run generate-types && tsc",
    "dev": "npm run generate-types && tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "docs:serve": "typedoc --watch",
    "validate-schema": "npm run download-schema && swagger-parser validate schema/openapi.yaml",
    "integration-test": "./scripts/run-integration-tests.sh",
    "integration-test:cross-language": "./scripts/run-integration-tests.sh",
    "integration-test:python": "python3 scripts/integration-test-python.py",
    "integration-test:typescript": "npx ts-node scripts/integration-test-typescript.ts",
    "integration-test:validate": "npx ts-node scripts/integration-test-validator.ts"
  },
  "keywords": [
    "orangetheory",
    "fitness",
    "api",
    "typescript",
    "supabase"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-cognito-identity": "^3.0.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.0.0",
    "@aws-sdk/signature-v4": "^3.0.0",
    "cognito-srp-helper": "^2.3.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "dotenv": "^17.2.1",
    "eslint": "^8.0.0",
    "js-yaml": "^4.1.0",
    "openapi-typescript": "^7.0.0",
    "swagger-parser": "^10.0.0",
    "typedoc": "^0.28.10",
    "typedoc-plugin-markdown": "^4.8.1",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "files": [
    "dist/"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  }
}
